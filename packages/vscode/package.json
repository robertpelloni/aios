{
  "name": "aios-vscode",
  "displayName": "AIOS Plugin",
  "version": "0.1.0-skeleton",
  "publisher": "aios",
  "engines": {
    "vscode": "^1.85.0"
  },
  "main": "./dist/extension.js",
  "activationEvents": [
      "onCommand:aios.connect"
  ],
  "contributes": {
    "commands": [
      {
        "command": "aios.connect",
        "title": "AIOS: Connect to Hub"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "tsc -p ./",
    "package": "vsce package",
    "test": "echo \"No tests specified for vscode\" && exit 0"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/node": "20.x",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "socket.io-client": "^4.7.4"
  }
}
