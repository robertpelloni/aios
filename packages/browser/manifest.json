{
  "manifest_version": 3,
  "name": "Super AI Plugin Browser Ext",
  "version": "1.0",
  "description": "Browser Client for Super AI Plugin Hub",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "http://localhost:3000/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
